library(tidyverse)


wages <- read_csv(file = "wage_data (1).csv")

wages %>% 
  filter(FamilySize < 7) %>% 
  group_by(Age, FamilySize) %>% 
  summarise(median = median(Earnings)) %>% 
  ggplot(aes(x = Age, y = median, color = factor(FamilySize))) +
  geom_line()


wages %>% 
  filter(FamilySize < 7) %>% 
  group_by(Age, Earnings)
